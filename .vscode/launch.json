{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome attach",
			"type": "chrome",
			"request": "attach",
			"port": 9222,
			"webRoot": "${workspaceFolder}/client/src",
			"sourceMapPathOverrides": {
				"webpack:///client/src/*": "${webRoot}/*"
			}
		},
		{
			"name": "Server attach",
			"type": "node",
			"request": "attach",
			"port": 9999,
			"restart": true,
			"protocol": "inspector",
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/backend/**/*.js"]
		},
		{
			"name": "Chrome app launch",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:3000/",
			"webRoot": "${workspaceRoot}/client/src",
			"userDataDir": false,
			"sourceMapPathOverrides": {
				"webpack:///client/src/*": "${webRoot}/*"
			}
		},
		{
			"name": "Chrome chat launch",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:3000/au?facultyId=5c6aaae0317e143a2b9775b2",
			"webRoot": "${workspaceRoot}/client/src",
			"userDataDir": false,
			"sourceMapPathOverrides": {
				"webpack:///client/src/*": "${webRoot}/*"
			}
		},
		{
			"name": "Chrome launch",
			"type": "chrome",
			"request": "launch",
			// "url": "http://localhost:3000",
			"url": "http://localhost:3000/au?faculty=5c6aaae0317e143a2b9775b2",
			"webRoot": "${workspaceRoot}/client/src",
			"userDataDir": false,
			"sourceMapPathOverrides": {
				"webpack:///client/src/*": "${webRoot}/*"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Server launch",
			"program": "${workspaceFolder}/index.js",
			"envFile": "${workspaceFolder}/backend/config/dev.js",
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/backend/**/*.js"],
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Debug attach",
			"processId": "${command:PickProcess}",
			"restart": true,
			"protocol": "inspector"
		}
	]
}
