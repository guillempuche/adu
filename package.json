{
    "name": "au",
    "version": "0.0.0",
    "description": "Asistente Personal Universitario",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "server": "nodemon --inspect=9999 index.js",
        "client": "npm run start --prefix client",
        "server-client": "concurrently \"npm run server\" \"npm run client\"",
        "debug": "concurrently \"npm run debug-server\" \"npm run debug-client\"",
        "debug-server": "nodemon --inspect index.js",
        "debug-client": "npm run start --prefix client",
        "debug-gulp": "npm run gulp",
        "gulp": "gulp vscode-sourcemap",
        "webhook": "ssh -o ServerAliveInterval=60 -R adu-dev:80:localhost:5001 serveo.net",
        "server-webhook": "concurrently \"npm run server\" \"npm run webhook\""
    },
    "author": "Guillem Puche Quesada",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.18.3",
        "botbuilder": "^3.15.0",
        "concurrently": "^3.6.0",
        "cookie-session": "^2.0.0-beta.3",
        "express": "^4.16.3",
        "lodash": "^4.17.10",
        "mongo-dot-notation": "^1.2.0",
        "mongoose": "^5.2.1",
        "morgan": "^1.9.1",
        "passport": "^0.4.0",
        "passport-auth0": "^1.1.0",
        "path-parser": "^4.1.1",
        "stackify-logger": "^2.0.2",
        "winston": "^3.2.1",
        "winston-stackify": "^1.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-concat": "^2.6.1",
        "gulp-sourcemaps": "^2.6.4",
        "nodemon": "^1.18.9"
    }
}
