{
	"name": "au",
	"version": "0.0.0",
	"description": "Assistent Universitari",
	"main": "serverless.yml",
	"author": "Guillem Puche Quesada",
	"license": "ISC",
	"scripts": {
		"test": "jest",
		"test:w": "jest --watch",
		"coverage": "jest --coverage",
		"server": "concurrently \"npm run server:tunnel\" \"npm run server:sls\"",
		"server:sls": "serverless offline",
		"server:tunnel": "ngrok start --config=ngrok.yml --all",
		"client": "npm run start --prefix src/primaryAdapters/website",
		"server:sls:deploy": "npm test && serverless deploy --stage dev",
		"server-client": "concurrently \"npm run server\" \"npm run client\"",
		"lint:e": "eslint . --cache",
		"lint:p": "prettier --write \"**/*.{json,yml,js}\""
	},
	"dependencies": {
		"@hapi/joi": "^16.1.4",
		"apollo-server-lambda": "^2.9.7",
		"auth0": "^2.20.0",
		"cloudinary": "^1.14.0",
		"cuid": "^2.1.6",
		"graphql": "^14.5.8",
		"jsonwebtoken": "^8.5.1",
		"jwks-rsa": "^1.6.0",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"mongo-dot-notation": "^1.2.0",
		"mongodb": "^3.4.1",
		"node-mailjet": "^3.3.1",
		"serverless": "^1.60.0",
		"serverless-domain-manager": "^3.3.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"apollo-server-core": "^2.9.9",
		"apollo-server-testing": "^2.9.9",
		"axios": "^0.19.0",
		"babel-eslint": "^10.0.3",
		"concurrently": "^3.6.1",
		"eslint": "^6.7.2",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-prettier": "^3.1.1",
		"faker": "^4.1.0",
		"husky": "^3.1.0",
		"jest": "^24.9.0",
		"lint-staged": "^9.5.0",
		"ngrok": "^3.2.5",
		"nock": "^11.7.0",
		"prettier": "^1.19.1",
		"serverless-jest-plugin": "^0.2.1",
		"serverless-offline": "^5.12.1"
	},
	"private": true
}
